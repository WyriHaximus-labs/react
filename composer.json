{
    "name": "recoil/react",
    "description": "Integrate Recoil with ReactPHP.",
    "require": {
        "php": "^7",
        "react/event-loop": "^0.4",
        "react/promise": "^2",
        "recoil/api": "dev-master",
        "recoil/recoil": "dev-124-split-api as 0.6.0"
    },
    "require-dev": {
        "eloquent/phony": "^0.13",
        "friendsofphp/php-cs-fixer": "^1",
        "hamcrest/hamcrest-php": "^2",
        "peridot-php/leo": "^1",
        "peridot-php/peridot-code-coverage-reporters": "^2",
        "peridot-php/peridot": "^1",
        "recoil/dev": "dev-master"
    },
    "autoload": { "psr-4": { "Recoil\\React\\": "src" } },
    "autoload-dev": {
        "psr-4": { "Recoil\\React\\": "test/src" }
    },
    "keywords": [
        "async",
        "asynchronous",
        "continuation",
        "coroutine",
        "event",
        "generator",
        "green",
        "multitasking",
        "promise",
        "react",
        "react",
        "thread"
    ],
    "homepage": "https://github.com/recoilphp/react",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
